{
  "name": "@maplat/ui",
  "version": "0.10.1",
  "description": "Maplat is the cool Historical Map/Illustrated Map Viewer.\nIt can transform each map coordinates with nonlinear but homeomorphic projection and makes possible that the maps can collaborate with GPS/accurate maps, without distorting original maps.",
  "main": "src/index.js",
  "module": "src/index.js",
  "files": [
    "src/",
    "dist/",
    "legacy/",
    "less/",
    "parts/",
    "fonts/",
    "locales/"
  ],
  "scripts": {
    "server": "webpack-cli serve --config ./webpack_config/webpack.config.standard.dev.js --mode=development --devtool=inline-source-map",
    "server:packed": "npm run freeze && webpack-cli serve --config ./webpack_config/webpack.config.packed.dev.js --mode=development --devtool=inline-source-map",
    "build": "npm run build:standard && npm run build:packed",
    "build:standard": "webpack --color --progress --config ./webpack_config/webpack.config.standard.js",
    "build:packed": "npm run freeze && webpack --config ./webpack_config/webpack.config.packed.js",
    "zip": "gulp zip",
    "test": "jest",
    "preversion": "npm run lint",
    "version": "npm run build && npm run zip && git add -A",
    "freeze": "node bin/freezer.js",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "lint:eslint": "eslint \"{src,spec}/**/*.js\" --fix",
    "lint:prettier": "prettier \"./{src,spec}/**/*.js\" --write",
    "git_switch": "gulp git_switch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/code4history/Maplat.git"
  },
  "keywords": [
    "warp",
    "GIS"
  ],
  "author": "Kohei Otsuka, Code for History",
  "license": "Maplat Limited License 1.1",
  "bugs": {
    "url": "https://github.com/code4history/Maplat/issues"
  },
  "homepage": "https://github.com/code4history/Maplat/wiki",
  "dependencies": {
    "@maplat/core": "^0.10.4",
    "@maplat/tin": "^0.9.3",
    "@turf/turf": "^6.5.0",
    "bootstrap.native": "^3.0.15",
    "caniuse-lite": "^1.0.30001239",
    "lodash.template": "^4.5.0",
    "ol": "^6.1.1",
    "ol-contextmenu": "^4.1.0",
    "swiper": "^6.7.1",
    "swiper4": "npm:swiper@^4.5.1",
    "weiwudi": "^0.1.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "concaveman": "https://github.com/grassick/concaveman#patch-1",
    "copy-webpack-plugin": "^6.4.1",
    "core-js": "^3.15.1",
    "css-loader": "^5.2.6",
    "css-minimizer-webpack-plugin": "^1.3.0",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-jest": "^24.3.6",
    "file-loader": "^6.2.0",
    "fs-extra": "^8.1.0",
    "gulp": "^4.0.2",
    "gulp-cli": "^2.3.0",
    "gulp-zip": "^4.2.0",
    "html-webpack-plugin": "^4.5.2",
    "jest": "^26.6.3",
    "jest-canvas-mock": "^2.3.1",
    "less": "^3.13.1",
    "less-loader": "^7.1.0",
    "lodash": "^4.17.21",
    "mini-css-extract-plugin": "^1.6.0",
    "prettier": "^2.3.1",
    "strip-whitespace-loader": "github:kochizufan/strip-whitespace-loader#fix_dq_npm",
    "terser-webpack-plugin": "^4.2.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.40.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2",
    "workbox-build": "^6.5.4",
    "workbox-cli": "^6.5.4",
    "workbox-core": "^6.5.4",
    "workbox-expiration": "^6.5.4",
    "workbox-precaching": "^6.5.4",
    "workbox-routing": "^6.5.4",
    "workbox-strategies": "^6.5.4",
    "workbox-webpack-plugin": "^6.5.4"
  },
  "resolutions": {
    "concaveman": "https://github.com/grassick/concaveman#patch-1"
  }
}
