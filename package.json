{
  "name": "@maplat/ui",
  "version": "0.10.6",
  "description": "Maplat is the cool Historical Map/Illustrated Map Viewer.\nIt can transform each map coordinates with nonlinear but homeomorphic projection and makes possible that the maps can collaborate with GPS/accurate maps, without distorting original maps.",
  "main": "dist/maplat-ui.umd.js",
  "module": "dist/maplat-ui.es.js",
  "types": "src/index.ts",
  "files": [
    "src/",
    "dist/",
    "legacy/",
    "less/",
    "less/",
    "assets/"
  ],
  "scripts": {
    "dev": "concurrently \"npm run watch:sw\" \"vite\"",
    "build:sw": "esbuild src/service-worker.js --bundle --outfile=public/service-worker.js --define:self.__WB_MANIFEST=[]",
    "watch:sw": "esbuild src/service-worker.js --bundle --outfile=public/service-worker.js --define:self.__WB_MANIFEST=[] --watch",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "lint:eslint": "eslint \"{src,spec}/**/*.js\" --fix",
    "lint:prettier": "prettier \"./{src,spec}/**/*.js\" --write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/code4history/Maplat.git"
  },
  "keywords": [
    "warp",
    "GIS"
  ],
  "author": "Kohei Otsuka, Code for History",
  "license": "Maplat Limited License 1.1",
  "bugs": {
    "url": "https://github.com/code4history/Maplat/issues"
  },
  "homepage": "https://github.com/code4history/Maplat/wiki",
  "dependencies": {
    "@maplat/core": "0.11.2",
    "@maplat/transform": "^0.3.0",
    "@turf/turf": "^6.5.0",
    "@types/page": "^1.11.9",
    "bootstrap.native": "^5.1.6",
    "qrcode": "^1.5.0",
    "swiper": "^4.5.0",
    "tiny-emitter": "^2.1.0",
    "weiwudi": "^0.1.4"
  },
  "peerDependencies": {
    "mapbox-gl": "^1.0.0 || ^2.0.0 || ^3.0.0",
    "maplibre-gl": "^3.0.0 || ^4.0.0",
    "ol": "^9.0.0 || ^10.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/ol": "^6.4.2",
    "@types/qrcode": "^1.5.0",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^9.2.1",
    "esbuild": "^0.27.1",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.0.0",
    "jsdom": "^24.0.0",
    "less": "^4.1.2",
    "ol": "^10.7.0",
    "prettier": "^3.0.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vite-plugin-pwa": "^1.2.0",
    "vitest": "^1.0.0",
    "workbox-build": "^7.4.0",
    "workbox-window": "^7.4.0"
  },
  "overrides": {
    "@babel/traverse": "7.23.2",
    "glob-parent": "5.1.2",
    "axios": "1.6.0",
    "minimist": "1.2.8"
  }
}